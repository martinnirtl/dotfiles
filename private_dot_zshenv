# ATTENTION do not edit this file directly (e.g. `vi ~/.zshenv`)

# CUSTOM ENVVARS
export DEV=$HOME/Development
export DEV_TOOLS=$DEV/tools
export DEV_TOOLS_BIN=$DEV/tools/bin
export BAT_THEME="Monokai Extended" # run `bat --list-themes` to get preview on themes
#export DOCKER_HOST="unix://$HOMEe/.local/share/containers/podman/machine/podman-machine-default/podman.sock"
#export FZF_BASE="$HOMEBREW_PREFIX/bin/fzf"
export GOPATH="$DEV_TOOLS/go"

# TODO look for better way - something that works automatically after brew install
export ZSH_COMPLETIONS_CACHE="$HOME/.oh-my-zsh/cache/completions"

# PATH EXTENSIONS
CARGO="$HOME/.cargo/bin"
GOBINS="$GOPATH/bin"
KREW="$HOME/.krew/bin"
export PATH="$PATH:$CARGO:$GOBINS:$KREW:$DEV_TOOLS_BIN"
